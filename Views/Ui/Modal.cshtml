    <script type="text/javascript">

    $(function () {
        $("#btnSave").click(function ()
        {
            var std = {};
            std.Name = '1'; 
            std.Address = '2';

            $.ajax({
                type: "POST",
                url: '@Url.Action("PushAction1")',
                data: '{std: ' + JSON.stringify(std) + '}',
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                success: function ()
                {
                    alert("Data has been pushed successfully.");
                },
                error: function () {
                    alert("Error while inserting data");
                }
            });
            return false;
        });
    });

    </script>
}
