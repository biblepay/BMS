@{ ViewData["Title"] = "Proposal List";
    ViewData["PageName"] = "bbp_proposallist";
    ViewData["Heading"] = "<i class='fal fa-vote-yea'></i> &nbsp;Proposal List";
    ViewData["Category1"] = "Governance";
    ViewData["PageDescription"] = "10% to Orphan Charity"; }
@section HeadBlock {
    <style>
        table.saved td {
            background:linear-gradient( rgba(0, 0, 0, 0.07), rgba(0, 0, 0, 0.14) );
            margin: 12px 12px 12px 12px;
            padding: 12px 12px 12px 12px;
        }

        table.saved td a:link {
               color: Maroon;
        }

        table.saved {
            border-collapse: separate;
            border-spacing: 10px;
            *border-collapse: expression('separate', cellSpacing = '10px');
        }
    </style>
}
<div class="fs-lg fw-300 p-5 bg-white border-faded rounded mb-g">

    @Html.Raw(ViewBag.ProposalList)

</div>
@section ScriptsBlock {}
